{
	"compilerOptions": {
		"module": "commonjs",
		"removeComments": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"allowSyntheticDefaultImports": true,
		"target": "es2017",
		"sourceMap": true,
		"outDir": "./dist",
		"baseUrl": "./",
		"incremental": true,
		"skipLibCheck": true,
		"strictNullChecks": true,
		"noImplicitAny": false,
		"strictBindCallApply": false,
		"forceConsistentCasingInFileNames": false,
		"noFallthroughCasesInSwitch": true,
		"noUnusedLocals": false,
		"noUnusedParameters": true,
		"lib": ["es6", "es2017", "esnext.asynciterable"],
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"noImplicitReturns": true,
		"strict": false,
		"esModuleInterop": true,
		"strictPropertyInitialization": true,
		"paths": {
			"@apis": ["src/apis"],
			"@common": ["src/common"],
			"@config": ["src/config"],
			"@logger": ["src/logger"],
			"@models": ["src/models"],
			"@entities": ["src/entities"],
			"@passport": ["src/passport"],
			"@jwt": ["src/jwt"],
			"@firebase": ["src/firebase"],
			"@middlewares": ["src/middlewares"],

			"@auth": ["src/apis/auth"],
			"@dummy": ["src/apis/dummy"],
			"@users": ["src/apis/users"],
			"@roles": ["src/apis/roles"],
			"@houses": ["src/apis/houses"],
			"@vehicles": ["src/apis/vehicles"],
			"@commodity-prices": ["src/apis/commodity-prices"],
			"@commodity-images": ["src/apis/commodity-images"],
			"@flash-sales": ["src/apis/flash-sales"],
			"@orders": ["src/apis/orders"],
			"@promotions": ["src/apis/promotions"],
			"@favorites": ["src/apis/favorites"],
			"@reviews": ["src/apis/reviews"],
			"@user-revertseens": ["src/apis/user-revertseens"],
			"@news": ["src/apis/news"],
			"@comments": ["src/apis/comments"],
			"@otps": ["src/apis/otps"],
			"@about-products": ["src/apis/about-products"],
			"@cadastral": ["src/apis/cadastral"],
			"@cgv-cgus": ["src/apis/cgv-cgus"],
			"@master-data": ["src/apis/master-data"],
			"@uploads": ["src/apis/uploads"]
		}
	},
	"exclude": ["./node_modules/**"],
	"include": ["./src/**/*.tsx", "./src/**/*.ts", "./__tests__/**/*.ts", "./global.d.ts", "src/models/commodity-price.schemats"]
}
