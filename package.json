{
  "name": "heroic-api",
  "version": "0.0.1",
  "description": "<p align=\"center\">   <a href=\"http://nestjs.com/\" target=\"blank\"><img src=\"https://nestjs.com/img/logo-small.svg\" width=\"120\" alt=\"Nest Logo\" /></a> </p>",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "node scripts/merge-prisma-schema.js && prisma generate && nest build",
    "postinstall": "prisma generate",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "debug": "cross-env NODE_ENV=development node --inspect=0.0.0.0:9229 node_modules/.bin/nest start --watch",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:dev:clean": "lsof -ti:3103 | xargs kill -9 2>/dev/null || true && cross-env NODE_ENV=development nest start --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "kill": "lsof -ti :3103 | xargs kill -9 2>/dev/null || true && pkill -f \"nest start\" 2>/dev/null || true",
    "kill:ngrok": "pkill -f ngrok || lsof -ti:4040 | xargs kill -9 2>/dev/null || true",
    "prisma:merge": "node scripts/merge-prisma-schema.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "npm run prisma:merge && prisma migrate dev",
    "prisma:studio": "node scripts/start-prisma-studio.js",
    "prisma:studio:ngrok": "node scripts/start-ngrok-prisma-studio.js",
    "prisma:pull": "npm run prisma:merge && prisma db pull",
    "prisma:push": "npm run prisma:merge && prisma db push",
    "prisma:sync:studio": "node scripts/sync-prisma-studio.js",
    "email:preview": "tsx scripts/preview-email.ts",
    "db:backup": "./scripts/backup_db.sh",
    "db:restore": "./scripts/restore_db.sh",
    "db:sync": "./scripts/sync-prisma.sh"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.11",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.11",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.11",
    "@nestjs/platform-socket.io": "^11.1.11",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/websockets": "^11.1.11",
    "@prisma/adapter-mariadb": "^7.1.0",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@prisma/config": "^7.2.0",
    "@react-email/components": "^1.0.3",
    "@react-email/render": "^2.0.1",
    "@supabase/supabase-js": "^2.89.0",
    "bcryptjs": "^3.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cookie-parser": "^1.4.7",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.19",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "mariadb": "^3.4.5",
    "mysql2": "^3.16.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "reflect-metadata": "^0.2.2",
    "resend": "^6.6.0",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.3",
    "swagger-ui-express": "^5.0.1",
    "telegraf": "^4.16.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.1.11",
    "@react-email/tailwind": "^2.0.3",
    "@react-email/text": "^0.1.6",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.5",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "express": "^5.2.1",
    "globals": "^17.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.7.3",
    "prisma": "^7.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "tailwindcss": "^4.1.18",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "webpack": "^5.104.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://Phan-Thanh-Ha@github.com/PhanHa17/Heroic-api.git"
  },
  "bugs": {
    "url": "https://github.com/PhanHa17/Heroic-api/issues"
  },
  "homepage": "https://github.com/PhanHa17/Heroic-api#readme"
}
